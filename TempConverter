import java.util.Scanner;
public class TempConverter {
	public static void main (String[] args)
	{		
		   Scanner keyboard = new Scanner(System.in);
		   System.out.println("Enter the Celsius degrees : ");
		   int Celsius = keyboard.nextInt();
		   double Fahrenheit;
		   Fahrenheit=32+(Celsius*9.0/5.0);
		   System.out.println(Celsius+" degrees in Celsius equal to "+Fahrenheit+" degrees Fahrenheit ");
	}
}
